<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.webxert.seefgouser.Home"
    tools:showIn="@layout/app_bar_home">


    <fragment xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:map="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.webxert.seefgouser.MapsActivity" />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_margin="20dp"
        android:background="@android:color/transparent"
        android:orientation="vertical">

        <FrameLayout
            android:id="@+id/warehouse_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/colorPrimary"
            android:padding="12dp">

            <TextView
                android:id="@+id/warehouse_1TV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Select Warehouse 1"
                android:textColor="@color/textColor"
                android:textSize="18sp" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/warehouse_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@color/colorPrimary"
            android:padding="12dp">

            <TextView
                android:id="@+id/warehouse_2TV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Select Warehouse 2"
                android:textColor="@color/textColor"
                android:textSize="18sp" />
        </FrameLayout>
    </LinearLayout>
    <!--<LinearLayout-->
    <!--android:orientation="vertical"-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="wrap_content">-->

    <!--&lt;!&ndash;<fragment&ndash;&gt;-->
    <!--&lt;!&ndash;android:id="@+id/place_autocomplete_fragment"&ndash;&gt;-->
    <!--&lt;!&ndash;android:name="com.google.android.gms.location.places.ui.PlaceAutocompleteFragment"&ndash;&gt;-->
    <!--&lt;!&ndash;android:layout_width="match_parent"&ndash;&gt;-->
    <!--&lt;!&ndash;android:layout_height="wrap_content">&ndash;&gt;-->

    <!--&lt;!&ndash;</fragment>&ndash;&gt;-->
    <!--</LinearLayout>-->


    <FrameLayout
        android:id="@+id/proceed_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="25dp"
        android:layout_marginEnd="20dp"
        android:layout_marginStart="20dp"
        android:background="@drawable/proceed_button_sheet"
        android:padding="12dp"
        android:visibility="invisible">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="Proceed"
            android:textColor="@color/textColor"
            android:textSize="16sp"
            android:textStyle="bold" />
    </FrameLayout>


</RelativeLayout>
